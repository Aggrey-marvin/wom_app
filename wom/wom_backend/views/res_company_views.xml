<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form_inherit" model="ir.ui.view">
        <field name="name">view.company.form.inherit</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="via_efris_client.ura_no_vat_view_company_form_inherited" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='currency_id']" position="after">
                <field name="default_image" widget="image" class="oe_avatar" nolabel="1"
                                style='padding-left: 15px; padding-bottom: 5px'/>
            </xpath>
        </field>
    </record>
</odoo>